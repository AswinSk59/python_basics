num=int(input("Enter the number: ")) 
flag= False
if(num==1):
        print("Number is a neither a prime number nor a composite number")
elif(num<1):
     print("Enter a valid input")
else: 
    for i in range(2, num):
           if num % i== 0:
                flag=True
                break
    if flag:
        print(num,"composite number")
    else:
        print(num,"prime number")
