print("enter marks of 5 subjects : ")
s1=int(input("mark of first subject : "))
s2=int(input("mark of second subject : "))
s3=int(input("mark of third subject : "))
s4=int(input("mark of fourth subject : "))
s5=int(input("mark of fifth subject : "))
total=s1+s2+s3+s4+s5
avg=total/5

print("average of 5 subjects is= ",avg)
